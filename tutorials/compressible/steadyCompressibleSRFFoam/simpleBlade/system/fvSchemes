FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default           none;

    ddt(rho,Urel)     steadyState;
    ddt(rho,i)        steadyState;
    ddt(psi,p)        steadyInertial phi rho 0.25;

    ddt(rho,k)        steadyState;
    ddt(rho,epsilon)  steadyState;

    Urel              steadyState;
    T                 steadyState;
    p                 steadyState;
}

gradSchemes
{
    default           Gauss linear;
}

divSchemes
{
    default           none;
    div(phi,Urel)    Gauss upwind;
    div(phi,i)       Gauss upwind;
    div(phid,p)      Gauss upwind;

    div(phi,k)       Gauss upwind;
    div(phi,epsilon) Gauss upwind;
    div((muEff*dev2(grad(Urel).T()))) Gauss linear;

    div(U,p)         Gauss linear;

}

laplacianSchemes
{
    default           Gauss linear corrected;
}

interpolationSchemes
{
    default           linear;
}

snGradSchemes
{
    default           corrected;
}

fluxRequired
{
    default           no;
    p;
}


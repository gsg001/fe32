solvers
{
    p
    {
        solver          amgSolver;
        cycle           W-cycle;
        policy          PAMG;
        nPreSweeps      2;
        nPostSweeps     2;
        groupSize       4;
        minCoarseEqns   4;
        nMaxLevels      100;
        scale           on;
        smoother        symGaussSeidel;

        minIter         1;
        maxIter         100;
        tolerance       1e-7;
        relTol          0.01;
    }

    U
    {
        solver           BiCGStab;
        preconditioner   ILU0;

        minIter          1;
        maxIter          1000;
        tolerance        1e-08;
        relTol           0;
    }
}

